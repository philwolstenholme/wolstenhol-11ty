{
  "name": "wolstenhol-11ty",
  "version": "1.0.0",
  "description": "An 11ty based personal website",
  "author": "Phil Wolstenholme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/philwolstenholme/wolstenhol-11ty"
  },
  "_moduleAliases": {
    "@config": ".config.js"
  },
  "scripts": {
    "clean": "rimraf '{dist,.11ty-vite}'",
    "dev:eleventy": "ELEVENTY_ENV=development ELEVENTY_EXPERIMENTAL=true eleventy",
    "dev": "NODE_ENV=development npm-run-all clean --parallel dev:* --print-label",
    "build:eleventy": "ELEVENTY_ENV=production ELEVENTY_EXPERIMENTAL=true eleventy",
    "build:reading-list": "cp -R src/submit-reading-item dist",
    "sprite": "svg-sprite-generate -d src/assets/images/icons -o src/assets/sprite.svg; sed -i.bak '1d' src/assets/sprite.svg; rm src/assets/sprite.svg.bak",
    "build": "NODE_ENV=production run-s clean sprite build:* --print-label",
    "watch:eleventy": "ELEVENTY_ENV=development ELEVENTY_EXPERIMENTAL=true eleventy --serve",
    "watch": "npm-run-all clean --parallel watch:*",
    "start": "npm run watch",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.0-canary.14",
    "@11ty/eleventy-cache-assets": "^2.3.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-vite": "^2.0.0-canary.2",
    "@11ty/eleventy-plugin-vue": "^0.7.3",
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@sergeymakinen/vite-plugin-html-minimize": "^1.0.4",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "cssnano": "^5.1.13",
    "deasync": "^0.1.28",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-prettier": "^3.4.1",
    "flat-cache": "^3.0.4",
    "got": "^11.8.5",
    "jsonfile": "^6.1.0",
    "luxon": "^1.28.0",
    "markdown-it": "^12.3.2",
    "markdown-it-imsize": "^2.0.1",
    "module-alias": "^2.2.2",
    "netlify-plugin-cache": "^1.0.3",
    "netlify-plugin-image-optim": "^0.4.0",
    "netlify-plugin-inline-critical-css": "^2.0.0",
    "netlify-plugin-subfont": "^6.0.0",
    "netlify-purge-cloudflare-on-deploy": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "postcss": "^8.4.16",
    "postcss-em-media-query": "^4.1.0",
    "postcss-import": "^14.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-pxtorem": "^6.0.0",
    "prettier": "^2.7.1",
    "tailwindcss": "^2.2.19",
    "vite-plugin-inspect": "^0.7.1"
  },
  "dependencies": {
    "@alpinejs/intersect": "^3.10.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/typography": "^0.3.1",
    "alpinejs": "^3.10.3",
    "async-alpine": "^0.5.3",
    "cheerio": "^1.0.0-rc.12",
    "cloudinary": "^1.31.0",
    "fetch-base64": "^2.1.2",
    "gmaps-url-signer": "^1.0.0",
    "js-yaml": "^3.14.1",
    "loadjs": "^4.2.0",
    "lodash": "^4.17.21",
    "lozad": "^1.16.0",
    "metascraper": "^5.30.4",
    "metascraper-description": "^5.30.4",
    "mini-svg-data-uri": "^1.4.4",
    "natural-orderby": "^2.0.3",
    "node-truncate-string": "0.0.2",
    "pluralize": "^8.0.0",
    "rss-parser": "^3.12.0",
    "spotify-web-api-node": "^5.0.2",
    "svg-sprite-generator": "0.0.7",
    "svgo": "^1.3.2",
    "tailwindcss-interaction-variants": "^5.0.0",
    "tailwindcss-scroll-snap": "^1.1.0",
    "timeago.js": "^4.0.2",
    "tldts": "^5.7.91",
    "twitter-text": "^3.1.0",
    "unique-random-array": "^2.0.0",
    "wicg-inert": "^3.1.2"
  }
}
